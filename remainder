def nbase(num,n):     #718,12
    remi=[]
    while num!=0:
        rem = num%n    #10      #11      #4
        remi.append(rem)
        num=num//n      #59    #4     #0
    result = ''
    for i in remi:
        if i>9:
            c = chr(i+55)
            result = result + c
    return result[::-1]
